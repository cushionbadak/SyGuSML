(set-logic PBE_SBVLIA)
(declare-datatypes ((List 0)) (((nil) (cons (head Int) (tail (List Int))))))
(synth-fun f ((x Int) (y Int)) Int
  ((I Int) (Ic Int))
  ((I Int (0 1 (Variable Int)
          (+ I I)
          (* Ic I)))
  (Ic Int (0 1 2 (- 1) (- 2)))))
(declare-var x Int)
(declare-var y Int)
(constraint (= (f x y) (* 2 (+ x y))))
(check-synth)